class Sunny 
{
	public static void main(String[] args) 
	{
		int n=5;
		int stars=n;
		int spaces=0;
		for(int i=1;i<=n;i++)
		{
			for(int j=1;j<=stars;j++)
			{
			System.out.print("*");
			}
			for(int k=1;k<=spaces;k++)
			{
			System.out.print(" ");
			}
			stars--;
			spaces++;
					System.out.println();
		}
	}
}
